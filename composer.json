{
  "name": "tagwalk/api-client-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides access to the Tagwalk API",
  "homepage": "https://www.tag-walk.com",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Florian Ajir",
      "email": "florian@tag-walk.com"
    }
  ],
  "require": {
    "php": "~7.1",
    "symfony/framework-bundle": "^4.1",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/security-bundle": "^4.1",
    "symfony/validator": "^4.1",
    "symfony/form": "^4.1",
    "symfony/asset": "^4.1",
    "symfony/translation": "^4.1"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.1",
    "phpunit/phpunit": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "Tagwalk\\ApiClientBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tagwalk\\ApiClientBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/simple-phpunit"
    ]
  }
}
